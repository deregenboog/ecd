{% for izKlant in izKlanten %}
    {{- "K" ~ izKlant.klant.id ~ tab -}}
    {{- (izKlant.izIntake and izKlant.izIntake.intakeDatum ? izKlant.izIntake.intakeDatum|date('d-m-Y') : '') ~ tab -}}
    {{- (izKlant.afsluitDatum ? izKlant.afsluitDatum|date('d-m-Y') : '') ~ tab -}}
    {{- izKlant.klant.geslacht ~ tab -}}
    {{- izKlant.klant.voornaam|tabless ~ tab -}}
    {{- izKlant.klant.tussenvoegsel|tabless ~ tab -}}
    {{- izKlant.klant.achternaam|tabless ~ tab -}}
    {{- izKlant.klant.geboortedatum|date('d-m-Y') ~ tab -}}
    {{- izKlant.klant.email|tabless ~ tab -}}
    {{- izKlant.klant.adres|tabless ~ tab -}}
    {{- izKlant.klant.postcode|tabless ~ tab -}}
    {{- izKlant.klant.plaats|tabless ~ tab -}}
    {{- izKlant.klant.mobiel|tabless ~ tab -}}
    {{- izKlant.klant.telefoon|tabless ~ tab -}}
    {{- (izKlant.izIntake and izKlant.izIntake.medewerker ? izKlant.izIntake.medewerker.naam : '') ~ tab -}}
    {%- spaceless %}
        {% set medewerkers = {} %}
        {% for izHulpvraag in izKlant.izHulpvragen %}
            {% if izHulpvraag.medewerker.naam not in medewerkers %}
                {% set medewerkers = medewerkers|merge([izHulpvraag.medewerker.naam]) %}
            {% endif %}
        {% endfor %}
    {% endspaceless -%}
    {{- medewerkers|join(', ') ~ tab -}}
    {{- izKlant.klant.werkgebied ~ tab -}}
    {{- izKlant.klant.postcodegebied ~ tab -}}
    {{- "n.v.t." ~ tab -}}
    {{- (izKlant.izIntake ? izKlant.izIntake.gezinMetKinderen : '') ~ tab -}}
    {{- izKlant.klant.geenPost ~ tab -}}
    {{- izKlant.klant.geenEmail ~ tab -}}
    {%- spaceless %}
        {% set izProjecten = {} %}
        {% for izHulpvraag in izKlant.izHulpvragen %}
            {% if izHulpvraag.izProject.naam not in izProjecten %}
                {% set izProjecten = izProjecten|merge([izHulpvraag.izProject.naam]) %}
            {% endif %}
        {% endfor %}
    {% endspaceless -%}
    {{- izProjecten|join(', ') }}
{% endfor %}
