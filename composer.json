{
    "name" : "deregenboog/ecd",
    "type" : "project",
    "config" : {
        "platform" : {
            "php" : "7.1"
        }
    },
    "autoload" : {
        "psr-4" : {
            "" : "src/",
            "App\\" : "app/"
        },
        "classmap" : [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev" : {
        "psr-4" : {
            "Tests\\" : "tests/"
        }
    },
    "require" : {
        "symfony/symfony" : "^3.4",
        "symfony/swiftmailer-bundle" : "^2.3",
        "symfony/monolog-bundle" : "^2.8",
        "sensio/distribution-bundle" : "^5.0",
        "sensio/framework-extra-bundle" : "^5.2",
        "incenteev/composer-parameter-handler" : "^2.0",
        "doctrine/orm" : "^2.5",
        "doctrine/doctrine-bundle" : "^1.6",
        "doctrine/doctrine-migrations-bundle" : "^1.2",
        "luxifer/doctrine-functions" : "^1.4",
        "knplabs/knp-paginator-bundle" : "^2.5",
        "jms/di-extra-bundle" : "^1.8",
        "symfony/assetic-bundle" : "^2.8",
        "vich/uploader-bundle" : "^1.4",
        "backup-manager/backup-manager" : "^1.1",
        "stof/doctrine-extensions-bundle" : "^1.2",
        "shivas/versioning-bundle" : "^1.4",
        "phpoffice/phpspreadsheet" : "^1.0",
        "beberlei/DoctrineExtensions" : "^1.0",
        "symfony/templating" : "^3.4",
        "symfony/translation" : "^3.4",
        "friendsofsymfony/ckeditor-bundle" : "^1.0",
        "tecnickcom/tcpdf" : "^6.2",
        "symfony/security-bundle": "^3.4",
        "ldaptools/ldaptools-bundle": "0.9.2@dev",
        "symfony/dotenv": "^3.4"
    },
    "require-dev" : {
        "friendsofphp/php-cs-fixer" : "^2",
        "doctrine/doctrine-fixtures-bundle" : "^2.3",
        "hautelook/alice-bundle": "^1.3",
        "doctrine/data-fixtures" : "^1.2",
        "phpunit/phpunit" : "^6.5",
        "liip/functional-test-bundle" : "^1.7",
        "symfony/phpunit-bridge" : "^3.2",
        "sebastian/phpcpd" : "^3.0",
        "nelmio/alice": "2.2.2"
    },
    "scripts" : {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra" : {
        "symfony-app-dir": "app",
        "symfony-bin-dir" : "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "app/webroot",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters" : {
            "file" : "app/config/parameters.yml"
        }
    }
}
