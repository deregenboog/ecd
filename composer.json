{
    "name" : "deregenboog/ecd",
    "type" : "project",
    "config" : {
        "platform" : {
            "php" : "5.6"
        }
    },
    "autoload" : {
        "psr-4" : {
            "" : "src/",
            "App\\" : "app/"
        },
        "classmap" : [
            "app/controllers",
            "app/models",
            "app/AppKernel.php",
            "app/AppCache.php",
            "app/app_controller.php",
            "app/app_error.php",
            "app/app_model.php",
            "cake/libs/"
        ],
        "exclude-from-classmap" : [
            "cake/libs/overloadable_php4.php",
            "cake/libs/controller/app_controller.php",
            "cake/libs/model/app_model.php",
            "cake/libs/view/helpers/html.php"
        ]
    },
    "autoload-dev" : {
        "psr-4" : {
            "Tests\\" : "tests/"
        }
    },
    "require" : {
        "symfony/symfony" : "^3.4",
        "symfony/swiftmailer-bundle" : "^2.3",
        "symfony/monolog-bundle" : "^2.8",
        "sensio/distribution-bundle" : "^5.0",
        "sensio/framework-extra-bundle" : "^3.0.2",
        "incenteev/composer-parameter-handler" : "^2.0",
        "doctrine/orm" : "^2.5",
        "doctrine/doctrine-bundle" : "^1.6",
        "doctrine/doctrine-migrations-bundle" : "^1.2",
        "luxifer/doctrine-functions" : "^1.4",
        "knplabs/knp-paginator-bundle" : "^2.5",
        "jms/di-extra-bundle" : "^1.8",
        "symfony/assetic-bundle" : "^2.8",
        "vich/uploader-bundle" : "^1.4",
        "backup-manager/backup-manager" : "^1.1",
        "stof/doctrine-extensions-bundle" : "^1.2",
        "shivas/versioning-bundle" : "^1.4",
        "phpoffice/phpspreadsheet" : "^1.0",
        "beberlei/DoctrineExtensions" : "^1.0",
        "symfony/templating" : "^3.4",
        "symfony/translation" : "^3.4",
        "egeloen/ckeditor-bundle" : "^6.0"
    },
    "require-dev" : {
        "friendsofphp/php-cs-fixer" : "^2",
        "doctrine/doctrine-fixtures-bundle" : "^2.3",
        "hautelook/alice-bundle" : "^1.3",
        "doctrine/data-fixtures" : "^1.2",
        "phpunit/phpunit" : "^5.7",
        "liip/functional-test-bundle" : "^1.7",
        "symfony/phpunit-bridge" : "^3.2",
        "sebastian/phpcpd" : "^3.0"
    },
    "scripts" : {
        "post-install-cmd" : [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd" : [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "extra" : {
        "symfony-bin-dir" : "bin",
        "incenteev-parameters" : {
            "file" : "app/config/parameters.yml"
        }
    }
}
