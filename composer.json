{
    "name" : "deregenboog/ecd",
    "type" : "project",
    "minimum-stability": "stable",
    "config" : {
        "platform" : {
            "php" : "7.4.32"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload" : {
        "psr-4" : {
            "App\\" : "src/",
            "AppBundle\\": "src/AppBundle",
            "BuurtboerderijBundle\\": "src/BuurtboerderijBundle",
            "ClipBundle\\": "src/ClipBundle",
            "DagbestedingBundle\\": "src/DagbestedingBundle",
            "ErOpUitBundle\\": "src/ErOpUitBundle",
            "GaBundle\\": "src/GaBundle",
            "HsBundle\\": "src/HsBundle",
            "InloopBundle\\": "src/InloopBundle",
            "IzBundle\\": "src/IzBundle",
            "LegacyBundle\\": "src/LegacyBundle",
            "MwBundle\\": "src/MwBundle",
            "OekBundle\\": "src/OekBundle",
            "OekraineBundle\\": "src/OekraineBundle",
            "PfoBundle\\": "src/PfoBundle",
            "ScipBundle\\": "src/ScipBundle",
            "TwBundle\\": "src/TwBundle",
            "UhkBundle\\": "src/UhkBundle",
            "VillaBundle\\": "src/VillaBundle"

        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Tests\\" : "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jtborger/ldaptools-bundle.git"
        }
    ],
    "require" : {
        "php": ">=7.4.25",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "backup-manager/backup-manager": "^1.1",
        "beberlei/doctrineextensions": "^1.0",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": "2.*",
        "doctrine/orm": "^2",
        "friendsofsymfony/ckeditor-bundle": "^2.2.0",
        "jtborger/ldaptools-bundle": "dev-master",
        "knplabs/knp-paginator-bundle": "^5",
        "luxifer/doctrine-functions": "^1.4",
        "pear/numbers_words": "^0.18.2",
        "phpoffice/phpspreadsheet": "^1.0",
        "sensio/framework-extra-bundle": "^5.2",
        "shivas/versioning-bundle": "^3",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dependency-injection": "^4.4",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.19",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/mailer": "^4.4",
        "symfony/monolog-bundle": "^3",
        "symfony/security-bundle": "^4",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.8",
        "symfony/yaml": "4.4.*",
        "tecnickcom/tcpdf": "^6.2",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2",
        "vich/uploader-bundle": "^1",
        "zf1/zend-pdf": "^1.12"
    },
    "require-dev" : {
        "dama/doctrine-test-bundle": "^6.7",
        "doctrine/data-fixtures": "^1.5",
        "doctrine/doctrine-fixtures-bundle": "^3",
        "friendsofphp/php-cs-fixer": "^3",
        "hautelook/alice-bundle": "^2.9.0",
        "liip/functional-test-bundle": "^4",
        "liip/test-fixtures-bundle": "^2.4",
        "nelmio/alice": "^3",
        "phpunit/phpunit": "^7",
        "rector/rector": "^0.12.16",
        "sebastian/phpcpd": "^4",
        "symfony/debug": "4.4.*",
        "symfony/dotenv": "*",
        "symfony/phpunit-bridge": "^4.4",
        "symfony/web-profiler-bundle": "4.4.*",
        "theofidry/alice-data-fixtures": "^1.5.2"
    },

    "extra" : {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        },
        "symfony-app-dir": "app",
        "symfony-bin-dir" : "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "app/webroot",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters" : {
            "file" : "app/config/parameters.yml"
        }
    },

    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": [
            "symfony-cmd"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    }
}
