{
    "name" : "deregenboog/ecd",
    "type" : "project",
    "require" : {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "backup-manager/backup-manager": "^2.0",
        "beberlei/doctrineextensions": "^1.2",
        "doctrine/doctrine-migrations-bundle": "^2.1",
        "friendsofsymfony/ckeditor-bundle": "^2.2",
        "knplabs/knp-paginator-bundle": "^4",
        "ldaptools/ldaptools-bundle": "^0.9.2",
        "luxifer/doctrine-functions": "^1.6",
        "phpoffice/phpspreadsheet": "^1.10",
        "sensio/framework-extra-bundle": "^5.5",
        "shivas/versioning-bundle": "^3.2",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/yaml": "4.4.*",
        "tecnickcom/tcpdf": "^6.3",
        "twig/twig": "^2",
        "vich/uploader-bundle": "^1.11",
        "zf1/zend-pdf": "^1.12"
    },
    "require-dev" : {
        "doctrine/data-fixtures" : "^1",
        "doctrine/doctrine-fixtures-bundle" : "^3",
        "friendsofphp/php-cs-fixer" : "^2",
        "hautelook/alice-bundle" : "^2",
        "liip/functional-test-bundle" : "^4.0",
        "liip/test-fixtures-bundle" : "^1.7",
        "phpunit/phpunit" : "^7",
        "sebastian/phpcpd" : "^4.1",
        "symfony/debug-pack" : "*",
        "symfony/maker-bundle" : "^1.0",
        "symfony/profiler-pack" : "*",
        "symfony/test-pack" : "*",
        "symfony/web-server-bundle" : "4.4.*",
        "theofidry/alice-data-fixtures" : "^1.1"
    },
    "config" : {
        "preferred-install" : {
            "*" : "dist"
        },
        "sort-packages" : true
    },
    "autoload" : {
        "psr-4" : {
            "\\" : "src/"
        },
        "classmap" : [
            "src/Kernel.php"
        ],
        "psr-0" : {
            "" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "App\\Tests\\" : "tests/"
        }
    },
    "replace" : {
        "paragonie/random_compat" : "2.*",
        "symfony/polyfill-ctype" : "*",
        "symfony/polyfill-iconv" : "*",
        "symfony/polyfill-php71" : "*",
        "symfony/polyfill-php70" : "*",
        "symfony/polyfill-php56" : "*"
    },
    "conflict" : {
        "symfony/symfony" : "*"
    },
    "extra" : {
        "symfony" : {
            "allow-contrib" : false,
            "require" : "4.4.*"
        }
    }
}
