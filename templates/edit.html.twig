{% extends 'base.html.twig' %}
{% import 'html.macro.twig' as html %}

{% block content_top %}
    <h1>{{ entity_name|capitalize }} wijzigen</h1>
    {% if ( path(route_base~'index') ) %}
        <p>
            {{ html.link('Annuleren', path(route_base~'index'), 'cancel') }}
        </p>
    {% endif %}
{% endblock %}

{% block content %}
    {{ form(form) }}
{% endblock %}
