{{ tab -}}
{% if aantalBijeenkomsten > 1 %}{% for i in 1..(aantalBijeenkomsten-1) %}
    {{- "Bijeenkomst " ~ i ~ tab -}}
{% endfor %}{% endif %}
{{ "Bijeenkomst " ~ aantalBijeenkomsten }}
{% for deelname in deelnames %}
    {{- deelname.oekKlant ~ tab -}}
    {% if aantalBijeenkomsten > 1 %}{% for i in 1..(aantalBijeenkomsten-1) %}
        {{- " " ~ tab -}}
    {% endfor %}{% endif %}
    {{- " " }}
{% endfor %}
