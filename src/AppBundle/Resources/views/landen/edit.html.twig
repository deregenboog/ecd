{% extends 'base.html.twig' %}
{% import 'html.macro.twig' as html %}

{% block title %}
    {{ title }}
{% endblock %}

{% block subnavigation %}
    {% include '@App/subnavigation.html.twig' %}
{% endblock %}

{% block content_top %}
    <h1>{{ entity_name|capitalize }} wijzigen</h1>
    <p>
        {{ html.link('Annuleren', path(route_base~'index'), 'cancel') }}
    </p>
{% endblock %}

{% block content %}
    {{ form(form) }}
{% endblock %}
